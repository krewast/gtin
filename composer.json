    "homepage": "https://github.com/powa/gtin",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Joseph Earl",
            "email": "joseph@powa.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
      "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {"Powa\\Gtin\\": "gtin.php/src/Powa/Gtin"}
    },
    "autoload-dev": {
        "psr-4": {"Powa\\Gtin\\": "gtin.php/tests/Powa/Gtin"}
    },
    "scripts": {
        "test": "./gtin.php/phpunit.phar"
    }
